{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "chloegittraining"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"CosmosDbMongoDbApi1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDbApi1'"
		},
		"CosmosDbMongoDbApi2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbMongoDbApi2'"
		},
		"RestService1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RestService1'"
		},
		"AzureTableStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureTableStorage1'"
		},
		"CosmosDbMongoDbApi1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adftraining"
		},
		"CosmosDbMongoDbApi2_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adftraining"
		},
		"RestService1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://jsonplaceholder.typicode.com/posts"
		},
		"RestService1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "chloe"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "chloegittraining_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"CosmosDbMongoDbApi1_connectionString": {
						"value": "[parameters('CosmosDbMongoDbApi1_connectionString')]"
					},
					"CosmosDbMongoDbApi2_connectionString": {
						"value": "[parameters('CosmosDbMongoDbApi2_connectionString')]"
					},
					"RestService1_password": {
						"value": "[parameters('RestService1_password')]"
					},
					"AzureTableStorage1_connectionString": {
						"value": "[parameters('AzureTableStorage1_connectionString')]"
					},
					"CosmosDbMongoDbApi1_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDbApi1_properties_typeProperties_database')]"
					},
					"CosmosDbMongoDbApi2_properties_typeProperties_database": {
						"value": "[parameters('CosmosDbMongoDbApi2_properties_typeProperties_database')]"
					},
					"RestService1_properties_typeProperties_url": {
						"value": "[parameters('RestService1_properties_typeProperties_url')]"
					},
					"RestService1_properties_typeProperties_userName": {
						"value": "[parameters('RestService1_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}